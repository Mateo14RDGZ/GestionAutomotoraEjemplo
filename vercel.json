{
  "version": 2,
  "buildCommand": "bash build.sh",
  "outputDirectory": "frontend/dist",
  "installCommand": "echo 'Skipping default install'",
  "functions": {
    "api/index.js": {
      "includeFiles": "{backend/**,api/**}"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ]
}
