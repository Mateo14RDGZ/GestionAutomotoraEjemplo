{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm run install:vercel",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
